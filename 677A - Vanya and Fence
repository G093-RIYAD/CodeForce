#include <bits/stdc++.h>
using namespace std;

int main(){
    int n, b, count = 0;
    cin >> n >> b;
    int ar[n];
    for(int i=0; i<n;i++){
        cin >> ar[i];
    }
    for(int i=0; i<n; i++){
        if(ar[i]> b){
            count+=2;
        }
        else{
            count+=1;
        }
    }
    cout << count;
}
